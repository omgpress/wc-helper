{
  "name": "omgpress/wc-helper",
  "version": "0.1.2",
  "type": "wordpress-library",
  "description": "Library for developing WooCommerce extensions",
  "homepage": "https://omgpress.com/wc-helper",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "OmgPress",
      "email": "hello@omgpress.com",
      "homepage": "https://omgpress.com"
    }
  ],
  "keywords": [
    "wordpress",
    "woocommerce"
  ],
  "config": {
    "platform-check": false,
    "optimize-autoloader": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "OmgWcHelper\\": "inc/"
    }
  },
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.0.0"
  },
  "scripts": {
    "no-dev": "composer install --no-dev",
    "lint": "phpcs",
    "fix": "phpcbf"
  }
}
